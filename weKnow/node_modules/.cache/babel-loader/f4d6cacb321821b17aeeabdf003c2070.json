data.name;\r\n            this.email = res.data.email;\r\n            this.id = res.data.id;\r\n\r\n        }).catch(err => {\r\n            console.log(err.response);\r\n             this.$router.push({name: 'Users'});\r\n        })\r\n\r\n    },\r\n    data(){\r\n        return {\r\n            name: '',\r\n            email: '',\r\n            id: -1,\r\n            error: undefined,\r\n        }\r\n    },\r\n    methods: {\r\n        update(){\r\n\r\n            var req = {\r\n                headers: {\r\n                    Authorization: \"Bearer \" + localStorage.getItem('token')\r\n                }\r\n            }\r\n\r\n\r\n            axios.put(\"http://localhost:8686/user\",{\r\n                name: this.name,\r\n                email: this.email,\r\n                id: this.id\r\n            }, req).then(res => {\r\n                console.log(res);\r\n                this.$router.push({name: 'Users'});\r\n            }).catch(err => {\r\n                var msgErro = err.response.data.err;\r\n                this.error = msgErro;\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views"}]}